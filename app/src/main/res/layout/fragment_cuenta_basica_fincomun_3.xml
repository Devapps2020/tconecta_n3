<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/header_height"
        android:orientation="horizontal"
        android:gravity="center">

        <ImageView
            android:visibility="invisible"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="@dimen/_25sdp"
            android:src="@drawable/ic_user"
            android:tint="@color/colorAccent"
            android:layout_margin="@dimen/_10sdp"/>

        <TextView
            style="@style/Graphik_Medium"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Fincomún"
            android:textColor="@color/colorBimboGrayDark"
            android:textSize="@dimen/_16sdp" />

        <ImageView
            android:visibility="invisible"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="26dp"
            android:layout_margin="@dimen/_10sdp"
            android:src="@drawable/ic_fincomun" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_15sdp">

                    <TextView
                        style="@style/Signika_Semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:padding="0dp"
                        android:text="Ayúdanos a verificar  y completar tu informacion"
                        android:textColor="@color/colorBimboBlueDark"
                        android:textSize="@dimen/_20sdp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:paddingBottom="@dimen/_15sdp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/text_tipo_1"
                                style="@style/Graphik_Medium"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/_10sdp"
                                android:gravity="center"
                                android:text="INE Frente"
                                android:textColor="@color/colorGrayBlack"
                                android:textSize="@dimen/_14sdp" />

                            <ImageView
                                android:id="@+id/img_front"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="@dimen/_110sdp"
                                android:src="@drawable/ine_3"/>

                            <Button
                                android:id="@+id/btn_take_1"
                                style="@style/Graphik_Medium"
                                android:layout_width="@dimen/_125sdp"
                                android:layout_height="@dimen/button_height"
                                android:background="@drawable/background_selector_green"
                                android:text="Volver a tomar foto"
                                android:textAllCaps="false"
                                android:textColor="@drawable/button_text_color"
                                android:textSize="@dimen/_11sdp"
                                android:layout_marginTop="@dimen/_10sdp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/text_tipo_2"
                                style="@style/Graphik_Medium"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="INE Reverso"
                                android:textColor="@color/colorGrayBlack"
                                android:layout_margin="@dimen/_10sdp"
                                android:textSize="@dimen/_14sdp" />

                            <ImageView
                                android:id="@+id/img_revert"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="@dimen/_110sdp"
                                android:src="@drawable/ine_4"/>

                            <Button
                                android:id="@+id/btn_take_2"
                                style="@style/Graphik_Medium"
                                android:layout_width="@dimen/_125sdp"
                                android:layout_height="@dimen/button_height"
                                android:background="@drawable/background_selector_blue"
                                android:text="Volver a tomar foto"
                                android:textAllCaps="false"
                                android:textColor="@drawable/button_text_color"
                                android:textSize="@dimen/_11sdp"
                                android:layout_marginTop="@dimen/_10sdp"/>

                        </LinearLayout>

                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_5sdp">

                    <TextView
                        style="@style/Graphik_Regular"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:text="Habilitar edición"
                        android:textColor="@color/colorGrayBlack"
                        android:textSize="@dimen/_12sdp" />

                    <CheckBox
                        android:id="@+id/check_enable_edit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:buttonTint="@color/colorBimboBlueDark"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:paddingRight="@dimen/_15sdp"
                    android:paddingLeft="@dimen/_15sdp">

                    <LinearLayout
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:paddingTop="@dimen/_5sdp"
                        android:paddingBottom="@dimen/_5sdp">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="right"
                            android:text="Habilitar edición"
                            android:textColor="@color/colorGrayBlack"
                            android:textSize="@dimen/_12sdp" />

                        <CheckBox
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:buttonTint="@color/colorBimboBlueDark"/>

                    </LinearLayout>

                    <LinearLayout
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:paddingTop="@dimen/_5sdp"
                        android:paddingBottom="@dimen/_5sdp">

                        <CheckBox
                            android:id="@+id/checkBox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:buttonTint="@color/colorBimboBlueDark"/>

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="Autenticar la identificación"
                            android:textColor="@color/colorGrayBlack"
                            android:textSize="@dimen/_12sdp" />

                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/_20sdp">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nombre"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp"/>

                        <EditText
                            android:id="@+id/edt_name"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:digits="0123456789"
                            android:background="@drawable/background_edittext_green"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:gravity="right"
                            android:padding="0dp"
                            android:textStyle="bold"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp"
                            android:singleLine="true"/>

                        <TextView
                            android:id="@+id/text_error_name"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"/>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/_20sdp">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Apellido Paterno"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp"/>

                        <EditText
                            android:id="@+id/edt_surname_f"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/text_error_surname_f"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"/>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/_20sdp">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Apellido Materno"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp"/>

                        <EditText
                            android:id="@+id/edt_surname_m"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:digits="0123456789"
                            android:background="@drawable/background_edittext_green"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:gravity="right"
                            android:padding="0dp"
                            android:textStyle="bold"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp"
                            android:singleLine="true"/>

                        <TextView
                            android:id="@+id/text_error_surname_m"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"/>

                    </RelativeLayout>

                    <TextView
                        style="@style/Graphik_Regular"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Fecha de nacimiento"
                        android:gravity="left"
                        android:textColor="@color/colorBimboGrayLight1"
                        android:textSize="@dimen/_12sdp"
                        android:layout_marginRight="@dimen/_30sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_20sdp"/>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_45sdp"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:src="@drawable/ic_down"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginBottom="@dimen/_20sdp"
                            android:tint="@color/colorPrimaryDark"/>

                        <com.reginald.editspinner.EditSpinner
                            android:id="@+id/edt_birthday"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right|center_vertical"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:paddingRight="@dimen/_25sdp"
                            android:singleLine="false"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp" />

                        <TextView
                            android:id="@+id/text_error_birthday"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"/>

                        <LinearLayout
                            android:id="@+id/layout_fecha"
                            android:clickable="true"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"/>

                    </RelativeLayout>




                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingRight="@dimen/_15sdp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Calle"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp" />

                        <EditText
                            android:id="@+id/edt_street"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/text_error_street"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"
                            android:visibility="gone" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Número interior"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp" />

                        <EditText
                            android:id="@+id/edt_number_iside"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/text_error_number_iside"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"
                            android:visibility="gone" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Número exterior"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp" />

                        <EditText
                            android:id="@+id/edt_number_exterior"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/text_error_number_exterior"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"
                            android:visibility="gone" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Colonia"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp" />

                        <EditText
                            android:id="@+id/edt_suburd"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/text_error_suburd"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"
                            android:visibility="gone" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="CP"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp" />

                        <EditText
                            android:id="@+id/edt_cp"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/text_error_cp"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"
                            android:visibility="gone" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Estado"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp" />

                        <EditText
                            android:id="@+id/edt_state"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/text_error_state"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"
                            android:visibility="gone" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Graphik_Regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Municipio"
                            android:textColor="@color/colorBimboGrayLight1"
                            android:textSize="@dimen/_12sdp" />

                        <EditText
                            android:id="@+id/edt_municipio"
                            style="@style/Graphik_Regular"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/background_edittext_green"
                            android:digits="0123456789"
                            android:gravity="right"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textCursorDrawable="@drawable/cursor_green"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/text_error_municipio"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_alignParentBottom="true"
                            android:background="@color/colorRed"
                            android:visibility="gone" />

                    </RelativeLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:gravity="center">

                    <TextView
                        android:id="@+id/text_domicilio"
                        style="@style/Graphik_Regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/_12sdp"
                        android:gravity="left"
                        android:layout_margin="@dimen/_20sdp"
                        android:text="@string/clic_here">

                    </TextView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginBottom="@dimen/_20sdp">

                    <Button
                        android:id="@+id/btn_next3"
                        style="@style/Graphik_Medium"
                        android:layout_width="@dimen/button_width"
                        android:layout_height="@dimen/button_height"
                        android:background="@drawable/background_selector_green"
                        android:text="Siguiente"
                        android:textAllCaps="false"
                        android:textColor="@drawable/button_text_color"
                        android:textSize="@dimen/_14sdp" />

                </LinearLayout>

            </LinearLayout>

        </ScrollView>

    </LinearLayout>

</LinearLayout>